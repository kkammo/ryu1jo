<h1>Listing materials</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @materials.each do |material| %>
      <tr>
        <td><%= material.subject %></td>
        <td><%= link_to 'Show', material %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
