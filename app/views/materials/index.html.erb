<body>
<h1>내 자료 목록</h1>
<%= link_to '평가 자료 등록', new_material_path, :class => 'btn btn-primary pull-right' %>

<table class="table table-condensed">
  <thead>
    <tr>
      <th colspan="3">
        내용
      </th>
    </tr>
  </thead>

  <tbody>
    <% @materials.each do |material| %>
      <tr style="background-color: ghostwhite">
        <td><%= material.field %></td>
        <td><%= material.subject %></td>
        <td><%= material.developer.id %></td>
        <td><%= link_to 'Show', material %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</body>
