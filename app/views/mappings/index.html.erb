<h1>Listing mappings</h1>


<% @mappings.each do |mapping| %>
  <div class="panel panel-info">
    <div class="panel-heading">
      <h3 class="panel-title">
        피평가 그룹: <%= mapping.ratee_group.id %>, 
        평가 그룹: <%= mapping.rater_group.id %>
      </h3>
    </div>
    <div class="panel-body">
      <div class="row" style="background-color: white">
        <div class="col-md-6">
          <h4>피평가자</h4>
          <% mapping.ratee_group.applieds.each do |applied| %>
            <p> <%= applied.developer.name %> </p>
          <% end %>
        </div>
        <div class="col-md-6">
          <h4>평가자</h4>
          <% mapping.rater_group.selecteds.each do |selected| %>
            <p> <%= selected.developer.name %> </p>
          <% end %>
        </div>
    </div>
  </div>
<% end %>
