<body>
  <div id="page-wrap" >
		<h2> 개발자 관리 </h2>
    <br>

		<table class="table table-condensed">
     	<thead>
   	   	<tr>
     	   	<th colspan="3">
            이름
     	   	</th>
          <th colspan="3">
            회사
          </th>
          <th colspan="3">
            부서
          </th>
   	   	</tr>
 	   	</thead>

  	  <tbody>
       	<% @developers.each do |developer| %>	        		
       		<tr style="background-color: ghostwhite">
  	       	<td colspan="3">
  	       		<%= developer.name %>
  	       	</td>
            <td colspan="3">
              <%= developer.cname %>
            </td>
            <td colspan="3">
              <%= developer.dname %>
            </td>
      	  </tr>
    	  <% end %>
  	  </tbody>
	  </table>
  </div>
</body>